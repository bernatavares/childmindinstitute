.g-homepage-container
  margin-bottom 10px

.g-logo-container
  display block
  width 100%
  min-height 65px
  padding 6px 12px
  font-size 14px
  line-height 1.42857143
  color #555
  background-color #fff
  background-image none
  border 1px solid #ccc
  border-radius 4px

.g-preview-logo
  margin-left 2%
  vertical-align inherit

.g-homepage-logo-btn
  width 90px
  display block
  margin-top 5px

.g-logo-btn-container
  display inline-block
